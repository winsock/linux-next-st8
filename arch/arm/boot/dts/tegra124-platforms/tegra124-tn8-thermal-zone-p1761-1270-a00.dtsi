	dummy_cool_dev: dummy-cool-dev {
			compatible = "dummy-cooling-dev";
			#cooling-cells = <2>; /* min followed by max */
	};

	thermal-zones {
		Tboard {
			polling-delay = <15000>; /* milliseconds */
			polling-delay-passive = <0>; /* milliseconds */
			thermal-sensors = <&tboard_thermistor>;

			trips {
				therm_est_trip: therm_est_trip {
					temperature = <40000>;
					type = "active";
					hysteresis = <1000>;
					writable;
				};
			};

			cooling-maps {
				map0 {
					trip = <&therm_est_trip>;
					cooling-device = <&dummy_cool_dev THERMAL_NO_LIMIT THERMAL_NO_LIMIT>;
					contribution = <100>;
					cdev-type = "therm_est_activ";
				};
			};

			thermal-zone-params {
				governor-name = "pid_thermal_gov";
			};
		};

		Tdiode {
			polling-delay = <0>; /* milliseconds */
			polling-delay-passive = <0>; /* milliseconds */
			thermal-sensors = <&tdiode_sensor>;
		};

		Battery {
			polling-delay = <0>; /* milliseconds */
			polling-delay-passive = <0>; /* milliseconds */
			thermal-sensors = <&tbat_temp>;
			status = "disabled";
		};

		PMIC-Die {
			polling-delay = <0>; /* milliseconds */
			polling-delay-passive = <0>; /* milliseconds */
			thermal-sensors = <&palams_thermal>;
			status = "disabled";

			trips {
				die_temp_thresh: hot-die {
					temperature = <120000>;
					type = "active";
					hysteresis = <0>;
				};
			};

			cooling-maps {
				map0 {
					trip = <&die_temp_thresh>;
					cooling-device = <&dummy_cool_dev THERMAL_NO_LIMIT
								THERMAL_NO_LIMIT>;
					contribution = <100>;
					cdev-type = "emergency-balanced";
				};
			};
		};
	};
