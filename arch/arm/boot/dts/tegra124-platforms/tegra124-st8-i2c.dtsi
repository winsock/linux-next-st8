#include "tegra124-st8-i2c-sensors.dtsi"
#include "tegra124-st8-i2c-battery.dtsi"
#include "tegra124-st8-i2c-palmas_tps65913.dtsi"

/ {
    // eMMC I2C? DSI DDC?
    i2c@0,7000c500 {
        #address-cells = <0x1>;
        #size-cells = <0x0>;
        compatible = "nvidia,tegra124-i2c";
        reg = <0x0 0x7000c500 0x0 0x100>;
        interrupts = <0x0 0x5c 0x4>;
        scl-gpio = <0x7 0xd9 0x0>;
        sda-gpio = <0x7 0xda 0x0>;
        nvidia,memory-clients = <0xe>;
        status = "okay";
        clock-frequency = <0x61a80>;
    };

    // HDMI DDC
    hdmi_ddc: i2c@0,7000c700 {
        #address-cells = <0x1>;
        #size-cells = <0x0>;
        compatible = "nvidia,tegra124-i2c";
        reg = <0x0 0x7000c700 0x0 0x100>;
        interrupts = <0x0 0x78 0x4>;
        scl-gpio = <0x7 0xac 0x0>;
        sda-gpio = <0x7 0xad 0x0>;
        nvidia,memory-clients = <0xe>;
        status = "okay";
        clock-frequency = <0x186a0>;
    };

    i2c@0,7000d100 {
        #address-cells = <0x1>;
        #size-cells = <0x0>;
        compatible = "nvidia,tegra124-i2c";
        reg = <0x0 0x7000d100 0x0 0x100>;
        interrupts = <0x0 0x3f 0x4>;
        nvidia,memory-clients = <0xe>;
        status = "okay";
        clock-frequency = <0x61a80>;
    };
};
